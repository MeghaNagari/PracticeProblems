public class QuickSortByMe {
	static void quickSort(int[] a, int l, int h) 
	{
		if (l < h) {
			int p = (l + h) / 2;

			int j = l;

			for (int i = 0; j < h; i++)

			{

				while (a[j] >= a[p])

				{

					if (j != h)

						j++;
					else break;

				}

				if (j <= h )

					{
					
					if(a[j]<a[h])
					
					{int t = a[i];

					a[i] = a[j];

					a[j]= t;
					}}

				if (j != h)

					j++;

			}

			quickSort(a, l, p - 1);

			quickSort(a, p + 1, h);
		}
		
		for(int i=0;i<a.length;i++)
		{
			System.out.print(a[i]+" ");
		}
		System.out.println();
		
	}

	

	public static void main(String args[]) {


		int a[] = { 11, 16, 2, 8, 1, 4, 9, 7 };

		quickSort(a, 0, 7);

	}

}
