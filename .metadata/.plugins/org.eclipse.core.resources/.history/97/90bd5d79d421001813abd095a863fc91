package datastructures;

public class ReferenceVarAsFormalParam {
	
	public static void main(String[] args)
	{
		
		Employee e2=new Employee();
		e2.seteId("44");
				e2.seteName("actual param");
				e2.setSal("50000");
				System.out.println(e2);
				
				
				
				referenceReflection(e2);
				
				System.out.println(e2);
				
				valueRelfection(e2.geteId());
					
					
			}

			private static void valueRelfection(String e) {
				
				Employee e1 = null;
				e1.seteId(e);
				
				
				
				
				
				// TODO Auto-generated method stub
				
			}

			private static void referenceReflection(Employee e) {
				// TODO Auto-generated method stub
				
				
				e.seteId("774");
				e.setSal("1000000");
				
				
		
		
		
		
		
		
	}
	
	
	

}
