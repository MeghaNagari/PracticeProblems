public class QuickSortByMe{ 
   static void quickSort(int[] a,int h,int l)
{if(l<h)
{    int p=(l+h)/2;
    
int j=l;
  
  for(int i=0;j<h;i++)
  
  {
    
    while(a[j]>a[p]&&a[j]!=a[p])
    
    {
      
      if(j!=h)
  
          j++;
  
      }

        if(j<h)
 
       swap(a[i],a[j]);
   
     if(j!=h)
  
      j++;
    
    
        
        
        
        
    }
    
quickSort(a,l,p-1);
  
  quickSort(a,p+1,h);
}}
   
static void swap(int a,int b)

    {
      
  int t=a;
     
   a=b;
  
      b=t;

    }
    
   
 public static void main(String args[]) {
       
 int a[]={11,16,2,8,1,4,9,7};
        
        
       
 quickSort(a,0,7);
        
            
}

}
