
public class InsertionSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int index=0;

		int a[]={3,4,2,6,5,1};
		for(int i=1;i<=5;i++)
		{
			if(i==1)
			{
				if(a[i]<a[i-1])
				{
					int t=a[i];
					a[i]=a[i-1];
					a[i-1]=t;
					
				}
			}
			
			else
			{
				if(a[i]<a[i-1])
				{
					int j=i-1;
					
					while(a[i]<a[j])
					{
					 index=j--;
					 if(j==0)
						 break;
					
					}
						
							int t=a[i];
							for(int k=i;k>index;k--)
							{
								a[i]=a[i-1];
							}
							
							
							
						
						
						
						
						
						
						
						
					}
					
					
					
				}
			}
			
			
		
		for(int i=1;i<=5;i++)
		System.out.println(a[i]);
		
		
	}

}
